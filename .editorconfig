# EditorConfig helps developers define and maintain consistent
# coding styles between different editors and IDEs
# editorconfig.org

root = true

[*]
charset = utf-8
end_of_line = crlf
insert_final_newline = true
indent_style = space
indent_size = 2
trim_trailing_whitespace = true

[*.css]
block_comment_start = /*:::
block_comment = *
block_comment_end = :::*/

[*.svg]
insert_final_newline = false
